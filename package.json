{
  "name": "m2p-dump",
  "version": "0.0.1",
  "description": "A tool for viewing output from ffprobe command run over OOYALA provider's MPEG2 PS files",
  "main": "index.js",
  "scripts": {
    "lint": "xo",
    "test": "npm run lint && node index.js",
    "start": "DEBUG=oo,m2p node index.js"
  },
  "keywords": ["OOYALA"],
  "author": "Kuu Miyazaki",
  "license": "MIT",
  "dependencies": {
    "config": "^1.24.0",
    "debug": "^2.5.2",
    "ffprobe": "^1.1.0",
    "ffprobe-static": "^2.0.0",
    "node-curl-download": "0.0.2",
    "ooyala-api": "^0.5.2",
    "through2-parallel": "^0.1.3"
  },
  "devDependencies": {
    "xo": "^0.17.1"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "private": true,
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "no-eval": 0,
      "camelcase": 0,
      "babel/new-cap": 0
    }
  }
}
